<h2>Sprint  <%= @sprint.number %></h2>
<br />

<p>
  <b>Start date:</b>
  <%= @sprint.start_date %>
</p>

<p>
  <b>End date:</b>
  <%= @sprint.end_date %>
</p>

<p>
  <b>Velocity:</b>
  <%= @sprint.velocity %>
</p>

<p>
  <b>Description:</b>
  <%= @sprint.description %>
</p>

<h3>Related User Stories</h3>

<%= button_to 'Edit', edit_sprint_path(@sprint),
              class: "btn primary", method: :get %>
<%= button_to 'Sprint Overview', sprints_path,
              class: "btn primary", method: :get %>

